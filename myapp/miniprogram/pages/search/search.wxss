/* pages/search/search.wxss */
@import "/pages/foot/foot.wxss";
.container{
  height:300rPX;
  margin-bottom:15rpx;
}
.title{
  font-size:40rpx;
  /* font-weight:bold; */
  color:#ffffff;
  /* margin-top:30px; */
  margin-bottom:10rpx;
  background:#d6d4d4;
  width:100%;
  height:80rpx;
  line-height: 80rpx;
  text-align:center;
  letter-spacing: 10rpx;
  box-shadow:2rpx 1rpx 3rpx #d6d4d4;

}

.section{
  margin:10rpx;
  margin-top:150rpx;
}
.section_declare{
  color:#eb0d0d;
  font-size:18rpx;
}
.content{
  width:350rpx;
  height:52rpx;
  border:1rpx solid #d6d4d4;
  padding:5rpx;
  font-size:25rpx;
  display: inline-block;
  position:relative;
}
.placeholder {
  /* placeholder颜色  */
  color: #aab2bd;
  /* placeholder字体大小  */
  font-size: 18rpx;
  /* placeholder位置  */
  text-align: left;
}

.search{
  /* width:35rpx; */
  height:64rpx;
  font-size:20rpx;
  text-align:center;
  display:inline-block;
  color:#ffffff;
  font-weight: 200rpx;
  border-radius:0rpx;
  line-height:64rpx;
  position: relative;
  top:1rpx;

  /* background-image: url(../images/search.png);
  background-size: 14rpx; */
  /* padding-top:10rpx; */
}

/* result */

.search_no{
  color:red;
}
.result_display{
    margin-top:50rpx;
    margin-left:200rpx;
    margin-right:20rpx;
    font-size:25rpx;
    color:darkgray;
}
.content_title{
  color:black;
  font-size:25rpx;
}
.content_result{
  margin-top:15rpx;
  margin-bottom:15rpx;
}